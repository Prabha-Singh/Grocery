{% assign itemStyle = '' %}{% if cart.item_count == 0 %}{% assign itemStyle = ' style="display:none;"' %}{% endif %}
{% assign itemEmpty = '' %}{% if cart.item_count == 0 %}{% assign itemEmpty = ' style="display:block;"' %}{% endif %}
{%- assign item_count = cart.item_count -%}
<header id="section-{{ section.id }}" class="hdr_four {% if section.settings.transparent_enable %}  header_transparent {% endif %} ">
  <div class="main_header   sticky-header">
    <div class="header_container">
      <div class="container-fluid">
        <div class="header_top_four">
          <div class="row align-items-center">  
            <div class="col-lg-4">
              {% if section.settings.enable_search %}
              <div class="search_container search_four">
                {% include 'search-bar' %}
              </div>
              {% endif %}
            </div>
            <div class="col-lg-4">
              <div class="logo logo_four">
                {%- include 'header-logo' -%}
              </div>
            </div>
            <div class="col-lg-4">
              <div class="header_account_area header_account_four">
                {% if section.settings.enable_settings_menu %}
                <div class="header_account-list top_links">
                  <a href="/account"> <i class="icon-users"></i> </a>
                  <!-- Dropdown Start -->
                  <ul class="dropdown_links">
                    {% assign account_menu = section.settings.account_menu_handle %}
                    {% for link in linklists[account_menu].links %}
                    <li>
                      <a href="{{ link.url }}">{{ link.title }}</a>
                    </li>
                    {% endfor %}
                  </ul>
                </div>
                {% endif %}
                {% if section.settings.enable_wishlist_menu %}
                <div class="header_account-list header_wishlist">
                  <a href="/pages/wishlist"><i class="icon-heart"></i></a>
                </div>
                {% endif %}

                {% if section.settings.enable_mini_cart_menu %}
                <div class="header_account-list  mini_cart_wrapper">
                  {%- include 'mini-cart' -%}
                </div>
                {% endif %}
              </div>
            </div>
                          <!-- Mobile Navigation Start Here -->
              <div class="site-mobile-navigation d-block d-lg-none">
                {%- if section.settings.mobile_menu == "default" -%}
                {% include 'mobile_main_menu' %}
                {%- elsif section.settings.mobile_menu == "offcanvas" -%}
                <div class="mobile-navigation-icon" id="mobile-menu-trigger">
                  <i></i>
                </div>
                {%- include 'offcanvas_menu' -%}
                {%- endif -%}
              </div>
              <!-- Mobile Navigation End Here -->
          </div>     
        </div>
        <div class="header_bottom_four">
          <div class="row">
            <!-- Menu Area Start Here -->
            <div class="col-lg-6 offset-lg-3 d-none d-lg-block main___m">
              {% if section.settings.main_menu_enable %}
              <div class="site-main-nav d-none d-lg-block">
                <nav class="site-nav center-menu">
                  {% include 'main-menu' %}
                </nav>
              </div>
              {% endif %}
            </div>
            <!-- Menu Area End Here -->
          </div>
        </div>
      </div>
    </div>
  </div> 
</header>
<!--header area end-->






